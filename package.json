{
  "name": "tdhttp",
  "version": "1.0.0",
  "description": "Finding outdated http references on Twoday",
  "main": "index.js",
  "scripts": {
    "antville": "node ./src/antville",
    "build": "node ./src/index --from=0 --to=106 --delay=300 --rebuild",
    "progress": "node ./src/utils/progress",
    "progress1": "node ./src/utils/progress --second=1",
    "stats": "node ./src/utils/domains",
    "test": "node ./src/index --to=4 --delay=50 --rebuild",
    "deploytestant": "node ./src/utils/upload --story=twodayantville",
    "deployprodant": "node ./src/utils/upload --build --story=twodayantville",
    "deploytestper": "node ./src/utils/upload --story=performance",
    "deployprodper": "node ./src/utils/upload --build --story=performance",
    "deploytestsel": "node ./src/utils/upload --story=select",
    "deployprodsel": "node ./src/utils/upload --build --story=select"
  },
  "author": "NeonWilderness",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "cheerio": "^1.0.0-rc.2",
    "simple-node-logger": "^0.93.37",
    "yargs": "^10.1.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.7",
    "@types/jquery": "^3.3.0",
    "@types/knockout": "^3.4.50",
    "dotenv-safe": "^4.0.4",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  }
}
